{
  "title": "カスタムキーバインド",
  "rules": [
    {
      "description": "方向キー",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "p",
            "modifiers": { "mandatory": ["control"] }
          },
          "to": [{ "key_code": "up_arrow" }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "n",
            "modifiers": { "mandatory": ["control"] }
          },
          "to": [{ "key_code": "down_arrow" }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "b",
            "modifiers": { "mandatory": ["control"] }
          },
          "to": [{ "key_code": "left_arrow" }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": { "mandatory": ["control"] }
          },
          "to": [{ "key_code": "right_arrow" }]
        }
      ]
    },
    {
      "description": "一般",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "m",
            "modifiers": { "mandatory": ["control"] }
          },
          "to": [{ "key_code": "return_or_enter" }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": ["control"],
              "optional": ["shift"]
            }
          },
          "to": [{
            "key_code": "return_or_enter",
            "modifiers": ["shift"]
          }]
        }
      ]
    }
  ]
}
